{
  "entry": "src/app/all/all.js",
  "extraBabelPlugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "ignoreMomentLocale": true,
  "theme": "./src/app/all/theme.js",
  "html": {
    "template": "./src/app/all/index.ejs"
  },
  "publicPath": "/",
  "disableDynamicImport": true,
  "hash": true,
  "es5ImcompatibleVersions": true,
  "extraBabelIncludes": [
    "node_modules/save-svg-as-png"
  ]
}
