{
  "name": "project-x",
  "version": "0.0.1",
  "description": "Project X",
  "private": true,
  "scripts": {
    "setup": "node ./scripts/setup.js config/all.webpackrc",
    "setup:ml": "node ./scripts/setup.js config/ml.webpackrc",
    "setup:graph": "node ./scripts/setup.js config/graph.webpackrc",
    "precommit": "npm run lint-staged",
    "start": "cross-env ESLINT=none roadhog dev",
    "start:no-proxy": "cross-env NO_PROXY=true ESLINT=none roadhog dev",
    "start:it": "cross-env SERVER=http://localhost:8080 ESLINT=none roadhog dev",
    "build": "cross-env ESLINT=none roadhog build",
    "build:all": "npm run setup && npm run build",
    "build:ml": "npm run setup:ml && npm run build",
    "build:graph": "npm run setup:graph && npm run build",
    "dev-build": "cross-env COMPRESS=none ESLINT=none roadhog build",
    "dev-build:all": "npm run setup && npm run dev-build",
    "dev-build:ml": "npm run setup:ml && npm run dev-build",
    "dev-build:graph": "npm run setup:graph && npm run dev-build",
    "release": "node ./scripts/build.js",
    "dev-release": "node ./scripts/build.js -dev",
    "site": "roadhog-api-doc static && gh-pages -d dist",
    "analyze": "cross-env ANALYZE=true roadhog build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "test": "roadhog test",
    "test:component": "roadhog test ./src/components",
    "test:all": "node ./tests/run-tests.js"
  },
  "dependencies": {
    "@antv/data-set": "^0.9.6",
    "@babel/polyfill": "^7.0.0",
    "antd": "^3.9.3",
    "babel-runtime": "^6.9.2",
    "bizcharts": "^3.2.4",
    "bizcharts-plugin-slider": "^2.1.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "create-react-class": "^15.6.3",
    "downloadjs": "^1.4.7",
    "dva": "^2.4.0",
    "dva-loading": "^2.0.5",
    "enquire-js": "^0.2.1",
    "fastclick": "^1.0.6",
    "identicon.js": "^2.3.3",
    "js-cookie": "^2.2.0",
    "jshint": "^2.9.6",
    "jsonpath": "^1.0.0",
    "keymaster": "^1.6.2",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.0",
    "lucio-tree-transfer": "^1.0.0-beta.1",
    "moment": "^2.22.2",
    "namor": "^1.1.1",
    "npm": "^5.10.0",
    "numeral": "^2.0.6",
    "object-hash": "^1.3.0",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "rc-drawer-menu": "^1.1.0",
    "react": "^16.5.2",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^5.1.0",
    "react-container-query": "^0.11.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.5.2",
    "react-draggable": "^3.0.5",
    "react-highlight": "^0.12.0",
    "react-jsonschema-form": "^1.0.4",
    "react-splitter-layout": "^3.0.1",
    "react-table": "^6.8.6",
    "save-svg-as-png": "^1.4.6",
    "url-polyfill": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-dva-hmr": "^0.4.1",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.6.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.0",
    "husky": "^1.0.1",
    "lint-staged": "^7.3.0",
    "mockjs": "^1.0.1-beta3",
    "pro-download": "^1.0.1",
    "redbox-react": "^1.6.0",
    "regenerator-runtime": "^0.12.1",
    "roadhog": "^2.4.5",
    "roadhog-api-doc": "^1.1.2",
    "rollbar": "^2.4.7",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "optionalDependencies": {
    "nightmare": "^2.10.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ]
}
